{
    "name": "rentpost/forte-payments-php",
    "type": "library",
    "description": "PHP API wrapper client for Forte's API v3 https://restdocs.forte.net/",
    "homepage": "https://github.com/rentpost/forte-payments-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Thomason",
            "email": "jacob@rentpost.com"
        },
        {
            "name": "Samual Anthony",
            "email": "sam@expertcoder.io"
        }
    ],
    "require": {
        "php":                          ">=8.0",

        "psr/log":                      "~1.0 || ~2.0 || ~3.0",

        "doctrine/common":              "~2.8 || ~3.0",
        "guzzlehttp/guzzle":            "~6.3",
        "monolog/monolog":              "~1.24 || ~2.0",

        "symfony/expression-language":  "^5.4 || ^6.0",
        "symfony/property-access":      "^5.4 || ^6.0",
        "symfony/property-info":        "^5.4 || ^6.0",
        "symfony/serializer":           "^5.4 || ^6.0",
        "symfony/validator":            "^5.4 || ^6.0",
        "symfony/var-dumper":           "^5.4 || ^6.0",
        "symfony/yaml":                 "^5.4 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rentpost\\ForteApi\\": "src/",
            "Rentpost\\ForteApi\\Test\\": "test/"
        }
    }
}
